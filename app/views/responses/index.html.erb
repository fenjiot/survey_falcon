<h2>Responses</h2>

<ul>
  <% @survey.questions.each do |question| %>
    <li>
      <%= question.style.body %>
      <ul>
        <% question.responses.each do |question_response| %>
          <li>
            <%= question_response.answer.body %>
          </li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>
